{
  "name": "crypto_wallet_manager",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "setup": "npm run migration:run && npm run seed:run",
    "setup-fly": "npm run migration:run && npm run seed:run && node dist/main.js",
    "cleanup": "rimraf node_modules dist package-lock.json",
    "start:local": "cross-env NODE_ENV=development nest start --watch",
    "start:dev": "cross-env NODE_ENV=development node dist/main",
    "start:test": "cross-env NODE_ENV=test node dist/main",
    "start:qa": "cross-env NODE_ENV=qa node dist/main",
    "start:uat": "cross-env NODE_ENV=uat node dist/main",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:debug": "nest start --debug --watch",
    "migration:typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./src/ormconfig-migrations.ts",
    "migration:run": "npm run migration:typeorm migration:run",
    "migration:revert": "npm run migration:typeorm migration:revert",
    "migration:show": "npm run migration:typeorm migration:show",
    "migration:create": "npm run migration:typeorm migration:create -- -n",
    "seed:typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./src/ormconfig-seeds.ts",
    "seed:run": "npm run seed:typeorm migration:run",
    "seed:create": "npm run seed:typeorm migration:create -- -n",
    "seed:revert": "npm run seed:typeorm migration:revert",
    "seed:show": "npm run seed:typeorm migration:show",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.26.0",
    "dotenv": "^10.0.0",
    "express-basic-auth": "^1.2.0",
    "express-rate-limit": "^5.5.1",
    "express-useragent": "^1.0.15",
    "fastify-swagger": "^4.13.1",
    "helmet": "^4.6.0",
    "mysql": "^2.18.1",
    "object-hash": "^2.2.0",
    "pascal-case": "^3.1.2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "postgresql": "^0.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "@core/enum": "<rootDir>/_core/enum/index",
      "@core/decorator": "<rootDir>/_core/decorator/index",
      "@core/interface": "<rootDir>/_core/interface/index",
      "@core/strategy": "<rootDir>/_core/strategy/index",
      "@core/utils": "<rootDir>/_core/utils/index",
      "@core/htttp.service": "<rootDir>_core/http-service/index",
      "@auth/(.*)": "<rootDir>/auth/$1",
      "@user/(.*)": "<rootDir>/user/$1",
      "@refresh-token/(.*)": "<rootDir>/refresh-token/$1",
      "@device/(.*)": "<rootDir>/device/$1",
      "@employee/(.*)": "<rootDir>/employee/$1",
      "@crypto/(.*)": "<rootDir>/crypto/$1"
    }
  }
}
